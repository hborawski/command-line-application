{
  "name": "command-line-application",
  "version": "0.1.4",
  "description": "A helpful wrapper around command-line-args and command-line-usage",
  "main": "dist/index.js",
  "umd:main": "dist/command-line-application.umd.production.js",
  "module": "dist/command-line-application.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "hipstersmoothie/command-line-application",
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "release": "auto shipit"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "auto": "^7.0.7",
    "husky": "^2.3.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "tsdx": "^0.5.11",
    "tslib": "^1.9.3",
    "typescript": "^3.5.0-dev.20190518"
  },
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^5.0.5",
    "io-ts": "^1.8.6"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  }
}
